<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_inicio_administrador"
    tools:context="keaplus.com.co.sistemaencuesta.Actividades.InicioAdministrador">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="INICIO ADMINISTRADOR"
        android:id="@+id/textViewTituloInicioAdmin"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp"
        android:textColor="#1557f0"
        android:textStyle="bold"
        android:gravity="center" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewNombreUsuarioLogueado"
        android:layout_below="@+id/textViewTituloInicioAdmin"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="left"
        android:height="40dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generar Reporte"
        android:id="@+id/buttonGenerarReporte"
        android:layout_below="@+id/textViewNombreUsuarioLogueado"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewUno"
        android:layout_below="@+id/buttonGenerarReporte"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDos"
        android:layout_below="@+id/textViewUno"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewTres"
        android:layout_below="@+id/textViewDos"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewCuatro"
        android:layout_below="@+id/textViewTres"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewCinco"
        android:layout_below="@+id/textViewCuatro"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewSeis"
        android:layout_below="@+id/textViewCinco"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewSiete"
        android:layout_below="@+id/textViewSeis"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:height="40dp" />

</RelativeLayout>
